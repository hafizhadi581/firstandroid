<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/iv_q171"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.831"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView8"
        app:srcCompat="@drawable/buttonnext" />

    <TextView
        android:id="@+id/tv_q172"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Does your child try to get you to watch him or her?"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_q171" />

    <TextView
        android:id="@+id/tv_q171"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="SEVENTEENTH QUESTION"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide_mq171"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="100dp" />

    <ScrollView
        android:id="@+id/scrollView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guide_mq171">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/linear_nq1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


                <CheckBox
                    android:id="@+id/cb_watch_me"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q173"
                    app:layout_constraintHorizontal_bias="0.484"
                    app:layout_constraintStart_toStartOf="@+id/guide_q172"
                    app:layout_constraintTop_toBottomOf="@+id/tv_yq171" />

                <CheckBox
                    android:id="@+id/cb_watch_babble"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q173"
                    app:layout_constraintStart_toStartOf="@+id/guide_q172"
                    app:layout_constraintTop_toBottomOf="@+id/cb_watch_me" />

                <CheckBox
                    android:id="@+id/cb_praise"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q173"
                    app:layout_constraintStart_toStartOf="@+id/guide_q172"
                    app:layout_constraintTop_toBottomOf="@+id/cb_watch_babble" />

                <CheckBox
                    android:id="@+id/cb_watch_look"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q173"
                    app:layout_constraintStart_toStartOf="@+id/guide_q172"
                    app:layout_constraintTop_toBottomOf="@+id/cb_praise" />

                <TextView
                    android:id="@+id/tv_yq171"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:gravity="center|left"
                    android:text="For Example, how he/she would try to get you to watch him/her"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q173"
                    app:layout_constraintStart_toStartOf="@+id/guide_q171"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tv_yq172"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="30dp"
                    android:gravity="center|left"
                    android:text="Say “Look!” or “Watch me!”?"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q172"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/guide_q171"
                    app:layout_constraintTop_toBottomOf="@+id/tv_yq171" />

                <TextView
                    android:id="@+id/tv_yq173"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:gravity="center|left"
                    android:text="Babble or make a noise to get you to watch what he/she is doing?"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q172"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="@+id/guide_q171"
                    app:layout_constraintTop_toBottomOf="@+id/tv_yq172" />

                <TextView
                    android:id="@+id/tv_yq174"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:gravity="center|left"
                    android:text="Look at you to get praise or comment?"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q172"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="@+id/guide_q171"
                    app:layout_constraintTop_toBottomOf="@+id/tv_yq173" />

                <TextView
                    android:id="@+id/tv_yq165"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:gravity="center|left"
                    android:text="Keep looking to see if you are looking?"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/guide_q172"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/guide_q171"
                    app:layout_constraintTop_toBottomOf="@+id/tv_yq174" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guide_q171"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_begin="20dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guide_q172"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_begin="300dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guide_q173"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_begin="350dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>